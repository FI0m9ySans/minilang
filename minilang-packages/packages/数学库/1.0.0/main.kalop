// 数学库 v1.0.0
函数 最大值(甲, 乙) {
    如果 (甲 > 乙) {
        返回 甲;
    } 否则 {
        返回 乙;
    }
}

函数 最小值(甲, 乙) {
    如果 (甲 < 乙) {
        返回 甲;
    } 否则 {
        返回 乙;
    }
}

函数 绝对值(甲) {
    如果 (甲 < 0) {
        返回 -甲;
    } 否则 {
        返回 甲;
    }
}

函数 四舍五入(甲, 位数) {
    定义 系数 = 10 ^ 位数;
    返回 整数(甲 * 系数 + 0.5) / 系数;
}
